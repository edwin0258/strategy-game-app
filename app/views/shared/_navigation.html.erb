<%= link_to "Home", root_path %>
<% if current_user %>
	<%= link_to "Profile", user_path(current_user) %>
	<%= current_user.username %>
	<%= link_to "Logout", logout_path %>
<% else %>
	<%= link_to "Overview", root_path %>
	<%= link_to "Login", login_path %>
	<%= link_to "Register", register_path %>
<% end %>